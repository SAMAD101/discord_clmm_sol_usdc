{
  "name": "tgbot_clmm_sol_usdc",
  "version": "1.0.0",
  "description": "Telegram Bot Liquidity Management Bot for the SOL-USDC Pool on Raydium",
  "main": "index.ts",
  "repository": "https://github.com/SAMAD101/tgbot_clmm_sol_usdc",
  "author": "Sam <asamadans@gmail.com>",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format-check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/axios": "^0.14.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "@libsql/client": "^0.15.7",
    "@solana/web3.js": "^1.98.0",
    "axios": "^1.8.2",
    "cron": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "grammy": "^1.35.0",
    "zod": "^3.24.2"
  }
}
